{
    "long-press-seconds": 0.6,
    "event-keys": {
        "A": [
            "MEDIA_PLAY_PAUSE",
            "MEDIA_PLAY_PAUSE"
        ],
        "D": [
            "MEDIA_PLAY_PAUSE",
            "MEDIA_PLAY_PAUSE"
        ],
        "B": [
            "VOLUME_UP",
            "MEDIA_NEXT_TRACK"
        ],
        "C": [
            "VOLUME_DOWN",
            "MEDIA_PREV_TRACK"
        ]
    },
    "log": {
        "file": {
            "name": "debug.log",
            "max-bytes": 4096,
            "backup-count": 1,
            "encoding": "utf-8"
        },
        "format": "%(asctime)s [%(threadName)-12.12s] [%(levelname)-5.5s] %(message)s",
        "date-format": "%m-%d %H:%M:%S",
        "level": "debug"
    }
}
